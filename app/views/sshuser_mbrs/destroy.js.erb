<% if @ajax_div %>
    <% if @server %>
        $("#<%= @ajax_div %>").html("<%= escape_javascript( render(:partial => "sshusers/users_server", :locals => { :server => @server, :type => 'server' }) ) %>");
    <% else %>
        $("#<%= @ajax_div %>").html("<%= escape_javascript( render(:partial => "sshusers/users_server", :locals => { :server => @server_group, :type => 'servergrup' }) ) %>");
    <% end %>
<% end %>


